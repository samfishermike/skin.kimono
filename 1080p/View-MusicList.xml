<includes>
	<include name="AlbumListView">
		<control type="group">
			<visible>Control.IsVisible(50)</visible>
			<control type="group">
				<posy>84</posy>
				<visible>[IntegerGreaterThan(Container(50).NumItems,0) + !system.getbool(filelists.showparentdiritems)] | [IntegerGreaterThan(Container(50).NumItems,-1) + system.getbool(filelists.showparentdiritems)]</visible>
				<control type="group">
					<control type="image">
						<!-- Button focus slider -->
						<posx>1053</posx>
						<posy>495</posy>
						<width>798</width>
						<height>99</height>
						<texture border="8">default-button.png</texture>
						<aspectratio>stretch</aspectratio>
					</control>
					<animation condition="Container(50).Position(0)" effect="slide" start="0,0" end="0,-396" time="0" easing="inout" reversible="true">Conditional</animation>
					<animation condition="Container(50).Position(1)" effect="slide" start="0,0" end="0,-297" time="0" easing="inout" reversible="true">Conditional</animation>
					<animation condition="Container(50).Position(2)" effect="slide" start="0,0" end="0,-198" time="0" easing="inout" reversible="true">Conditional</animation>
					<animation condition="Container(50).Position(3)" effect="slide" start="0,0" end="0,-99" time="0" easing="inout" reversible="true">Conditional</animation>
					<!-- Animation for postion 4 does nothing, so removed-->
					<animation condition="Container(50).Position(5)" effect="slide" start="0,0" end="0,99" time="0" easing="inout" reversible="true">Conditional</animation>
					<animation condition="Container(50).Position(6)" effect="slide" start="0,0" end="0,198" time="0" easing="inout" reversible="true">Conditional</animation>
					<animation condition="Container(50).Position(7)" effect="slide" start="0,0" end="0,297" time="0" easing="inout" reversible="true">Conditional</animation>
				</control>
			</control>
			<control type="group">
				<animation type="TYPE" condition="Skin.HasSetting(tiltmusiclisticon)" reversible="false">
					<effect type="rotatey" start="-15" end="-15" time="0" />
					<effect type="slide" end="22,0" time="0" />
					<effect type="zoom" start="100" end="112" center="auto" time="0" tween="back" easing="out" />
				</animation>
				<control type="group">
					<visible>!Container.Content(addons)</visible>
					<control type="image">
						<posx>216</posx>
						<posy>147</posy>
						<width>627</width>
						<height>627</height>
						<texture>case-cd.png</texture>
						<aspectratio>keep</aspectratio>
						<visible>Skin.HasSetting(CDcase) + [Container.Content(songs) | Container.Content(albums)] + !SubString(ListItem.Icon,Default,Left)</visible>
					</control>
					<control type="image">
						<posx>285</posx>
						<posy>201</posy>
						<width>525</width>
						<height>525</height>
						<texture background="true" diffuse="mask-squ.png" fallback="DefaultAudio.png">$INFO[ListItem.Icon]</texture>
						<aspectratio>stretch</aspectratio>
					</control>
				</control>
				<control type="group">
					<visible>Container.Content(addons)</visible>
					<control type="image">
						<posx>285</posx>
						<posy>201</posy>
						<width>525</width>
						<height>525</height>
						<texture background="true" diffuse="mask-squ.png" fallback="DefaultAudio.png">$INFO[ListItem.Icon]</texture>
						<aspectratio>stretch</aspectratio>
					</control>
				</control>
			</control>
			<control type="group">
				<visible>[Container.Content(songs) | Container.Content(albums)] + !SubString(ListItem.Icon,Default,Left)</visible>
				<control type="image">
					<posx>46</posx>
					<posy>768</posy>
					<width>981</width>
					<height>9</height>
					<texture>separator.png</texture>
				</control>
				<control type="label">
					<!-- Artist heading -->
					<posx>186</posx>
					<posy>788</posy>
					<width>266</width>
					<height>38</height>
					<label>$LOCALIZE[31230]</label>
					<font>font11</font>
					<align>right</align>
				</control>
				<control type="label">
					<!-- Artist -->
					<posx>240</posx>
					<posy>788</posy>
					<width>770</width>
					<height>38</height>
					<label>$INFO[ListItem.Artist]</label>
					<include condition="Skin.HasSetting(autoscroll)">scrolltext</include>
					<font>font11</font>
				</control>
				<control type="label">
					<!-- Genre heading -->
					<posx>186</posx>
