					<posx>240</posx>
					<posy>862</posy>
					<width>770</width>
					<height>38</height>
					<label>$INFO[ListItem.Year]</label>
					<font>font11</font>
				</control>
				<control type="label">
					<!-- Rating heading -->
					<posx>186</posx>
					<posy>900</posy>
					<width>266</width>
					<height>38</height>
					<label>$LOCALIZE[563]:</label>
					<font>font11</font>
					<align>right</align>
				</control>
				<control type="image">
					<!-- Rating -->
					<posx>204</posx>
					<posy>900</posy>
					<width>225</width>
					<height>38</height>
					<texture>$VAR[MediaRating]</texture>
					<aspectratio>keep</aspectratio>
				</control>
				<control type="image">
					<posx>46</posx>
					<posy>951</posy>
					<width>981</width>
					<height>9</height>
					<texture>separator.png</texture>
				</control>
			</control>
			<control type="group">
				<include>SortLetterNotification</include>
				<control type="scrollbar" id="23">
					<posx>1875</posx>
					<posy>168</posy>
					<width>18</width>
					<height>832</height>
					<onleft>50</onleft>
					<animation condition="!Control.HasFocus(23) + !Container.Scrolling" effect="fade" start="100" end="0" time="200" reversible="true">Conditional</animation>
					<visible>Skin.HasSetting(sbar)</visible>
				</control>
			</control>
