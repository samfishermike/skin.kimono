	<controls>
		<control type="image" id="1">
			<!-- Pointer Image -->
			<width>48</width>
			<height>48</height>
			<texture>pointer.png</texture>
		</control>
		<control type="image" id="2">
			<!-- Pointer Focus Image -->
			<width>48</width>
			<height>48</height>
			<texture>pointer.png</texture>
		</control>
		<control type="image" id="3">
			<!-- Pointer Drag Image -->
			<width>48</width>
			<height>48</height>
			<texture>pointer-drag.png</texture>
		</control>
		<control type="image" id="4">
			<!-- Pointer Click Image -->
			<width>48</width>
			<height>48</height>
			<texture>pointer-drag.png</texture>
		</control>
